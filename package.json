  "dependencies": {
    "sitemap": "^8.0.0",
    "xml-formatter": "^3.6.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.1",
    "@antfu/utils": "^0.7.8",
    "@types/node": "^20.12.12",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^9.3.0",
    "eslint-plugin-format": "^0.1.1",
    "pkgroll": "^2.1.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  }
}
