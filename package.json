    "@antfu/eslint-config": "0.39.8",
    "@antfu/utils": "0.7.5",
    "@types/node": "18.16.19",
    "@vitest/coverage-c8": "0.31.4",
    "eslint": "8.45.0",
    "pkgroll": "1.10.0",
    "sitemap": "7.1.1",
    "typescript": "5.1.6",
    "vite": "4.4.4",
    "vitest": "0.31.4",
    "xml-formatter": "3.4.1"
  }
}
