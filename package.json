    "@antfu/eslint-config": "0.41.3",
    "@antfu/utils": "0.7.6",
    "@types/node": "20.8.6",
    "@vitest/coverage-v8": "0.34.6",
    "eslint": "8.51.0",
    "pkgroll": "1.11.1",
    "sitemap": "7.1.1",
    "typescript": "5.2.2",
    "vite": "4.4.11",
    "vitest": "0.34.6",
    "xml-formatter": "3.5.0"
  }
}
