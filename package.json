{
  "name": "vite-plugin-pages-sitemap",
  "version": "1.6.1",
  "description": "vite-plugin-pages based sitemap generator",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll",
