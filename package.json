{
  "name": "vite-plugin-pages-sitemap",
  "version": "1.5.0",
  "description": "vite-plugin-pages based sitemap generator",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll",
